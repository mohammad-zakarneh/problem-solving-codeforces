#include <iostream>
using namespace std;
int main()
{
    long n; cin >> n;
    int a, b; cin >> a >> b;
    
    long s = 0;
    for (int i = a; i <= n; i++) {
        int sum = 0;
        int j = i;
        while (j != 0) {
            sum += (j % 10);
            j /= 10;
           
        }
        if (sum >= a && sum <= b)s += i;
    }
    cout << s;
 
 
}
