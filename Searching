

#include <iostream>
using namespace std;
int main()

{
	long n; cin >> n;
	long* arr = new long[n];
	long long x;
	for (int i = 0; i < n; i++)
	{
		 cin >> x;
		arr[i] = x;
	}
	cin >> x;
	bool c = 0;
	for (int i = 0; i < n; i++) {
		if (arr[i] == x)
		{
			cout << i;
			c = 1;
			break;
			
		}
		
	}

if (c == 0)cout << "-1";

}

