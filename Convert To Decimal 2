 
#include <iostream>
using namespace std;
int main()
{
    int n,s; cin >> n;
    long long x;
    for (int i = 0; i < n; i++)
    {
        s = 0;
        cin >> x;
        while (x != 0) {
            s += x % 2;
            x /= 2;
 
        }
        long long z = 0;
          
        for (int j = 0; j < s; j++) {
            z = z * 2 + 1;
       }
        cout << z<<endl;
 
 
 
 
    }
 
 
 
}
