#include <iostream>
using namespace std;
 
int main()
{
    int t; cin >> t;
    long n;
    for (int i = 0; i < t; i++) {
        cin >> n;
        if (n == 0) {
            cout << 0 ;
        }
        while (n > 0) {
            cout << n % 10<<" ";
            n /= 10;
 
        }
        cout << endl;
    }
 
    
}
